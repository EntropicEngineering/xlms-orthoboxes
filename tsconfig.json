{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./typings/",
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es2017"
  },
  "include": [
    "./src/"
  ]
}