{
  "name": "xlms-orthoboxes",
  "version": "0.1.0",
  "private": true,
  "description": "Webapp for Orthoboxes on XLMS",
  "main": "index.js",
  "scripts": {
    "build": "npm run tsc && npm run rollup",
    "tsc": "tsc",
    "rollup": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.2",
    "mobx-react-devtools": "^4.2.15",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "simple-hid": "^1.0.0-rc5"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "jest": "^22.4.0",
    "rollup": "^0.56.2",
    "rollup-plugin-node-resolve": "^3.0.3",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./jest_setup.js"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true,
        "tsConfigFile": "./__test__/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EntropicEngineering/xlms-orthoboxes.git"
  },
  "author": "Benjamin Riggs (https://github.com/riggs/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EntropicEngineering/xlms-orthoboxes/issues"
  },
  "homepage": "https://github.com/EntropicEngineering/xlms-orthoboxes#readme"
}
